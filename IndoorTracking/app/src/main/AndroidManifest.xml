<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.foi.indoortracking">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>



    <activity
        android:name="hr.foi.indoortracking.Profile"
        android:label="@string/title_activity_display_message"
        android:parentActivityName="hr.foi.indoortracking.MainActivity" >
                <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="hr.foi.indoortracking.MainActivity" />
    </activity>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />

        <activity android:name=".SessionManager"></activity>
        <activity android:name=".MyMovements"></activity>
        <activity android:name=".HistoryAll"></activity>
    <activity android:name=".Profile"></activity>
</application>

</manifest>